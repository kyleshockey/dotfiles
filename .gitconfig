# dest: ~/.gitconfig

# Git Immersion aliases
[alias]
  co = checkout
  cob = checkout -b
  ci = commit
  st = status
  br = branch
  cl = clone
  di = diff
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  a = add
  p = push
  pl = pull
  amend = commit --amend

[core]
	editor = "nano -OScim"
	
